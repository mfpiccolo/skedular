<article id="contacts">
  <section>
    <ul class='list' role='list'>
      {{#each controller}}
      <li class='comp' {{action 'goTo' 'contacts.show' this}} data-goto="some-id">
        <div>
          <h3>{{username}}</h3>
        </div>
        <aside>
          <h4>{{display_name}}</h4>
          <span class='nav'></span>
        </aside>
        </li>
      {{/each}}
    </ul>
  </section>
</article>
